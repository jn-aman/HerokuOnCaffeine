  function ValidateEmail(mail) 
{
 

 var mailformat = /^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/;
 if(mail.match(mailformat))
  {
    return (true)
  }
    
    return (false)
}
function validateWebsite(mail) 
{
 

 var mailformat = /^((https?|ftp|smtp):\/\/)?(www.)?[a-z0-9]+(\.[a-z]{2,}){1,3}(#?\/?[a-zA-Z0-9#]+)*\/?(\?[a-zA-Z0-9-_]+=[a-zA-Z0-9-%]+&?)?$/;
 if(mail.match(mailformat))
  {
    return (true)
  }
    
    return (false)
}

